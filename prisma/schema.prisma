generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PageSpeedResult {
  id              Int      @id @default(autoincrement())
  url             String
  device          String
  performanceScore Float
  fcp             Float
  lcp             Float
  cls             Float
  tti             Float
  tbt             Float
  createdAt       DateTime @default(now())
}